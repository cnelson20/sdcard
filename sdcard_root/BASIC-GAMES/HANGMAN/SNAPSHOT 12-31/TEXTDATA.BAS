 INPUT "TEXT FILE TO CONVERT";F$ OPEN 2,8,2,F$+",S,R" OPEN 4,8,4,"@:"+F$+".BAS,S,W" EL=ST COUNT=0 LOOP:     IF EL<>0 THEN CLEANUP     PRINT# 4,"DATA ";     LINPUT# 2, L$     CHK=ST     L$=CHR$(34)+L$+CHR$(34)     PRINT# 4, L$     PRINT ".";     COUNT=COUNT+1     IF CHK=0 THEN LOOPCLEANUP: PRINT# 4 PRINT# 4, "REM LINES CONVERTED: ";COUNT PRINT:PRINT COUNT;" LINES CONVERTED.":PRINT CLOSE 4 CLOSE 2