BIGWINANIM:   F$="ANIMS/ANIMPAL.BIN":GOSUB FILEEXISTS   F1%=FE%   F$="ANIMS/ANIM16.BIN":GOSUB FILEEXISTS   F2%=FE%   F$="ANIMS/ANIM50.BIN":GOSUB FILEEXISTS   F1%=F1% AND F2% AND FE%   GOSUB PAL2BLACK   RECT 0,0,319,239,5   IF F1% THEN BVLOAD "ANIMS/ANIMPAL.BIN",8,1,$FA00   P1=254:R%=13:G%=13:B%=13:GOSUB RGBPAL   P1=15:GOSUB RGBPAL   P1=252:R%=6:G%=8:B%=2:GOSUB RGBPAL   P1=10:GOSUB RGBPAL:P1=14:GOSUB RGBPAL   P1=253:R%=5:G%=11:GOSUB RGBPAL   CHAR 4,158,156,CHR$($0C)+"A REPRIEVE !!!"   CHAR 4,170,254,CHR$($0C)+"LETS GET OUT OF HERE...."   CHAR 160,170,156, CHR$($0C)+" .. EXIT .. STAGE RIGHT !!"   RECT 0,234,319,239,252   RECT 0,175,319,233,153   I=1   IF NOT F1% THEN THEEND;ANIMLOOP:   F$=STR$(I):F$=RIGHT$(F$,LEN(F$)-1)   F$="ANIMS/ANIM"+F$+".BIN"   BVLOAD F$,8,0,$DAC0   I=I+1:IF I>50 THEN THEEND   GET X$:IF X$=CHR$(27) THEN THEEND   GOTO ANIMLOOPTHEEND:    RECT 0,175,319,233,156:GOSUB PAL2BLACK    RECT 0,0,319,239,$10:GOSUB SETDEFPAL   RETURN