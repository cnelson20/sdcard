REM BASIC WITH NO LINE NUMBERS!
REM GOTO WITH LABELS...
REM ==========================
SCREEN 0
N=5
F=128:E=0:K=4^N
FOR X=1 TO N
  U=F:F=-E/2:E=U/2
NEXT X
SCREEN 128:RECT0,0,319,239,0
X=100:Y=80
PSET X,Y,1
FOR M=1 TO K
  XO=X:YO=Y
  Z=M:X=X+F:Y=Y+E
  REM DRAW WITH RANDOM COLORS
  LINE XO,YO,X,Y,RND(0)*255
  FOR A=0 TO 1
    Z=Z/2
    Q=INT(Z)
    A=Z-Q
  NEXT A
  W=Q/2:V=INT(Q/2)
  IF W=V THEN A=1:GOTO SKIP
  A=-1
SKIP:
  U=F
  F=A*E
  E=-A*U
NEXT M
END:
GOTO END