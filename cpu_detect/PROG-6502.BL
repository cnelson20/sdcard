POKE $30F,0
SYS $FFED
SW=PEEK($30D)
SH=PEEK($30E)
SC=INT(SW/2)

DATA "DETECTED: 6502 CPU"
DATA "YOU CAN UPGRADE AT MOUSER.COM"
DATA "ITEM # W65C816S6PG-14"
DATA "ZZ"

PRINT " ";RPT$(195,SW-2)
FOR I=1 TO 2 STEP 0
    READ A$
    IF A$="ZZ" THEN DONE
    CENTER=SC-INT(LEN(A$)/2)
    PRINT TAB(CENTER);A$
NEXT
DONE:
    PRINT " ";RPT$(195,SW-2)
    END
