A.REG= $030CX.REG= $030DY.REG= $030EC.REG= $030FSETLFS = $FFBACHKOUT = $FFC9MCIOUT = $FEB1VERA.LOWADDR  = $9F20VERA.MIDADDR  = $9F21VERA.HIGHADDR = $9F22VERA.DATA0 = $9F23VERA.DATA1 = $9F24VERA.CTRL = $9F25SCREEN.SIZE = $12C00SCREEN $80RECT 0,0,319,239,$10FOR X = 1 TO 100    X1=RND(0)*280 + 4    Y1=RND(0)*220 + 4    X2=RND(0)*318 + 1    Y2=RND(0)*236 + 1    RECT X1,Y1,X2,Y2, RND(0)*255 + 1    LINE X2,Y1,X1,Y2, RND(0)*255 + 1NEXT XLINE 0,0,319,239,$3BRECT 10,10,20,20,$3ARECT 20,20,30,30,$39RECT 30,30,40,40,$38RECT 40,40,65,70,$77CHAR 60,60,78,"THIS IS THE GRAPHICS SCREEN"COLOR 1:PRINT "SAVING"S = TIGOSUB SAVESCREENE = TIT = E-SPRINT "DONE IN:";T;" JIFFIES."GKEY:GET X$:IF X$="" THEN GKEYSCREEN $80COLOR $8,0PRINT "PRESS ANY KEY TO RELOAD SCREEN"GKEY2:GET X$:IF X$="" THEN GKEY2BVLOAD "MYSCREEN.BIN",8,0,0ENDSAVESCREEN:F$="MYSCREEN.BIN"FF$="@:"+F$ + ",P,W"DOIT:    PRINT "ENTRY"    I=0    OPEN 2,8,2,FF$    POKE X.REG, 2    SYS CHKOUT    POKE VERA.CTRL,0    POKE VERA.LOWADDR,$00    POKE VERA.MIDADDR,$00    POKE VERA.HIGHADDR, %00010000SLOOP:      BYTES=SCREEN.SIZE-I      IF BYTES>255 THEN BYTES=0      POKE A.REG,BYTES      POKE X.REG, $23      POKE Y.REG, $9F      POKE C.REG, $01      SYS MCIOUT      I=I+PEEK(X.REG)+(PEEK(Y.REG)*256)      IF I<SCREEN.SIZE THEN SLOOP   CLOSE 2   POKE X.REG,3   SYS CHKOUT   RETURNVADDR: