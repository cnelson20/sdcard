 #CONTROLCODES 1

 GOSUB SETREGVARS
 POKE C.REG,1:SYS SET.MEM
 LAST.BANK = PEEK(A.REG)
 IF LAST.BANK=0 THEN LAST.BANK=256
 LAST.BANK = LAST.BANK - 1
 GOSUB GETFROMMENU

F$="HANGMAN.INI":GOSUB FILEEXISTS
IF NOT FE% THEN CREATINIFILE
PRINT "{CR}{WHT}   HANGMAN.INI {LIGHT GREEN}EXISTS {YEL}!"
PRINT "{CR}   CONTINUING WILL OVERWRITE IT {YEL}!{CR}"
PRINT "   {LIGHT GREEN}OVERWRITE {WHT}HANGMAN.INI {WHT} Y{GRN}/{WHT}N{LIGHT GREEN}?{WHT}"

GETCHOICE:
  GOSUB GETCHRX
  IF X$="N" OR X$="{ESC}" THEN ALLDONE
  IF X$<>"Y" THEN GETCHOICE

CREATINIFILE:
  PRINT "{CR}{YEL}WRITING{GRN}: {WHT}";F$;"{GRN}.";
  RESTORE INIDATA
  OPEN 4,8,4,F$+",S,W"
  FOR I = 1 TO 2114
      READ C:C$=CHR$(C)
      PRINT# 4,C$;
      PRINT ".";
  NEXT I
  PRINT "{CR}"
  CLOSE 4
  PRINT "{CR}{WHT}HANGMAN.INI {YEL}RESTORED.{WHT}{CR}"

ALLDONE:
  IF FROM.MENU THEN LOAD "CHECKFILES"
  PRINT "{CR}"
  PRINT "{WHT}   DONE{GRN}....{WHT}{CR}"
  END

GETCHRX:
   GET X$:X=ASC(X$)
   IF X>=193 AND X<=218 THEN X=X-128:X$=CHR$(X)
   IF X>=97 AND X<=122 THEN X=X-32:X$=CHR$(X)
   RETURN

FILEEXISTS:
   X$=F$:FE%=0:X$=X$+",S,R"
   OPEN 2,8,2,X$:CLOSE 2
   OPEN 15,8,15:INPUT#15,X,X$,A,B
   CLOSE 15:FE%=(X<=20):RETURN

GETFROMMENU:
  RA=PEEK(0):S$="":ADDR=$BF00:BANK LAST.BANK
  FOR I=0 TO 7:S$=S$+CHR$(PEEK(ADDR+I)):NEXT I
  FROM.MENU = (S$="HANGMENU")
  S$="TRIEDINI"
  ADDR=ADDR+8
  FOR I=0 TO 7
    POKE ADDR+I, ASC(MID$(S$,I+1,1))
  NEXT I
  BANK RA:RETURN

